    <problems>
        <localized-names>
            <key>ru</key>
            <value>$problem_title</value>
        </localized-names>
        <author>$author</author>
        <invocation-limits>
            <time-limit-millis>$limit_time</time-limit-millis>
            <idleness-limit-millis>10000</idleness-limit-millis>
            <memory-limit>$limit_memory</memory-limit>
            <output-limit>67108864</output-limit>
        </invocation-limits>
        $test_sets
        <max-source-size-bytes>262144</max-source-size-bytes>
        <author-solution-file2-compiler-id>
            <key>$solution</key>
            <value>python3_7_3</value>
        </author-solution-file2-compiler-id>
        <checker-settings>
            <standard-name>cmp_file</standard-name>
            <type>standard-checker</type>
            <invocation-limits>
                <time-limit-millis>10000</time-limit-millis>
                <idleness-limit-millis>10000</idleness-limit-millis>
                <memory-limit>268435456</memory-limit>
                <output-limit>268435456</output-limit>
            </invocation-limits>
        </checker-settings>
        <problem-type>problem-with-checker</problem-type>
        <test-file-type>text</test-file-type>
        <default-locale>ru</default-locale>
        <file-set>
            <input-file>input.txt</input-file>
            <output-file>output.txt</output-file>
            <redirect-stdin>true</redirect-stdin>
            <redirect-stdout>true</redirect-stdout>
        </file-set>
    </problems>
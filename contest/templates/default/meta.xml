<contest-meta>
    <name>$contest_name</name>
    <time-control-type>virtual</time-control-type>
    <start-time>0</start-time>
    $duration
    $problem_codes
    $problems
    <compilers>python3_7_3</compilers>
    <registration-allowance>$registration</registration-allowance>
    <starting-allowed>true</starting-allowed>
    <show-blind-results-after-contest-end>true</show-blind-results-after-contest-end>
    <contest-type>simple</contest-type>
    <monitor-visibility>participants-after-start</monitor-visibility>
    <monitor-time-format>hours-minutes</monitor-time-format>
    <report-settings>
        <report-visible>true</report-visible>
        <show-test-number>true</show-test-number>
        <test-only-samples>false</test-only-samples>
        <stop-on-first-fail>true</stop-on-first-fail>
        <stop-on-sample-fail>false</stop-on-sample-fail>
        <stop-on-first-fail-in-test-set>false</stop-on-first-fail-in-test-set>
        <use-ac-not-ok>false</use-ac-not-ok>
        <report-tokens-count>100</report-tokens-count>
        <use-lightweight-interface>false</use-lightweight-interface>
        <show-used-resources>all</show-used-resources>
        <show-verdict>all</show-verdict>
        <show-score>never</show-score>
        <show-input>never</show-input>
        <show-output>never</show-output>
        <show-answer>never</show-answer>
        <show-stderr>never</show-stderr>
        <show-checker-output>never</show-checker-output>
        <show-postprocessor-output>all</show-postprocessor-output>
    </report-settings>
    <source-size-limit>262144</source-size-limit>
    <monitor-plugin>acm</monitor-plugin>
    <max-problem-submissions-count>100</max-problem-submissions-count>
    <duplicate-submissions-allowed>false</duplicate-submissions-allowed>
    <teams-allowed>false</teams-allowed>
    <use-report-tokens>false</use-report-tokens>
    <token-settings>
        <report-visible>true</report-visible>
        <show-test-number>true</show-test-number>
        <test-only-samples>false</test-only-samples>
        <stop-on-first-fail>true</stop-on-first-fail>
        <stop-on-sample-fail>false</stop-on-sample-fail>
        <stop-on-first-fail-in-test-set>false</stop-on-first-fail-in-test-set>
        <use-ac-not-ok>false</use-ac-not-ok>
        <report-tokens-count>100</report-tokens-count>
        <use-lightweight-interface>false</use-lightweight-interface>
        <show-used-resources>never</show-used-resources>
        <show-verdict>never</show-verdict>
        <show-score>never</show-score>
        <show-input>never</show-input>
        <show-output>never</show-output>
        <show-answer>never</show-answer>
        <show-stderr>never</show-stderr>
        <show-checker-output>never</show-checker-output>
        <show-postprocessor-output>all</show-postprocessor-output>
    </token-settings>
    <allow-upsolving>true</allow-upsolving>
    <upsolving-allowance>allowed</upsolving-allowance>
    <frontend-visibilities>
        <participants>true</participants>
        <standings>true</standings>
        <submits>true</submits>
        <messages>true</messages>
        <problems>true</problems>
        <all-problems>false</all-problems>
        <use-single-submit-button>false</use-single-submit-button>
    </frontend-visibilities>
</contest-meta>